@model IEnumerable<Mars_Rover_Photos.Models.ImageList>
@{
    ViewBag.Title = "Home Page";
}



<table class="table">
    <tr>
        <th>
            Mars Rover Images
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                <img src="@item.img_src" alt="@item.img_src">

            </td>
            <td>
                @Html.ActionLink("Download : " + @item.img_src + " ? ", "Download", new { ImageName = @item.img_src })
            </td>
        </tr>
    }

</table>